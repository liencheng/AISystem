<Window x:Class="TableCheck.Window.Warning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TableCheck.Window"
        mc:Ignorable="d"
        Title="{DynamicResource str_common_Waring}" Height="600" Width="600" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" WindowStyle="None" AllowsTransparency="True" Background="#00000000">
    <Grid>
        <Image x:Name="Im_WarIcon" Margin="64,10,66,242" RenderTransformOrigin="0.5,0.5"/>
        <TextBlock x:Name="TB_Msg" HorizontalAlignment="Left" Margin="10,363,0,0" TextWrapping="Wrap"  Foreground="Red" FontSize="16" FontWeight="Bold" Text="" VerticalAlignment="Top" Width="572" Height="148"/>
        <Button x:Name="Bt_Continue" Content="{DynamicResource str_common_continue}" HorizontalAlignment="Left" Margin="46,531,0,0" VerticalAlignment="Top" Width="75" Click="Bt_Continue_Click"/>
        <Button x:Name="Bt_Cancel" Content="{DynamicResource str_common_cancle}" HorizontalAlignment="Left" Margin="475,531,0,0" VerticalAlignment="Top" Width="75" Click="Bt_Cancel_Click"/>
    </Grid>
</Window>
