<Window x:Class="TableCheck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TableCheck"
        mc:Ignorable="d"
        Title="{DynamicResource str_title}" Height="750" Width="1200">
    <Grid>
        <Viewbox Stretch="Fill">
            <TabControl x:Name="FunctionListTab" TabStripPlacement="Left" Margin="0,0,0,0" SelectionChanged="TabItemSelect">
            </TabControl>

        </Viewbox>
    </Grid>
</Window>
